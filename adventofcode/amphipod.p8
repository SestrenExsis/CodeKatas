-- helper functions

function cleanmap()
	-- clear map
	for y=3,13 do
		for x=1,14 do
			mset(x,y,0)
		end
	end
	-- process tiles
	for tile in all(_brd.tiles) do
		for dx=-1,1 do
			for dy=-1,1 do
				local x=tile.x+dx
				local y=tile.y+dy
				if dx==0 and dy==0 then
					mset(x,y,tile.typ)
				elseif mget(x,y)==0 then
					local typ=56
					if y<8 then typ=56
					elseif x==4 then typ=57
					elseif x==6 then typ=58
					elseif x==8 then typ=59
					elseif x==10 then typ=60
					end
					mset(x,y,typ)
				end
			end
		end
	end
	_dirty=false
end

function getamphipods(depth)
	local bag={}
	for d=1,depth do
		for i=1,4 do
			add(bag,i)
		end
	end
	local res={}
	for d=1,depth do
		for i=1,4 do
			local idx=1+flr(rnd(#bag))
			local amf=amphipod:new(
				2+2*i,
				5+d,
				bag[idx]
			)
			add(res,amf)
			bag[idx]=bag[#bag]
			deli(bag,#bag)
		end
	end
	return res
end
