-- main

function _init()
	menuitem(1,
		"load input",
		load_input
		)
	_data={}
	for row in all(_defaultdata) do
		add(_data,row)
	end
	_done=false
	_moving=false
	_mnrt=1
	_mxrt=7
	_mndn=1
	_mxdn=2
	_x=0
	_y=0
	_rt=3
	_dn=1
	_spd=1
	_shk=0
	_hits=0
	_cx=0 -- camera shake x
	_cy=0 -- camera shake y
	_debris={}
	-- generate a repeating
	-- quadrant of snowfields
	_qsz=32     -- quad size
	_siz=2*_qsz -- map size
	_spc=12 -- space between trees
	-- randomly generate a quadrant
	for r=0,_qsz-1 do
		for c=0,_qsz-1 do
			local idx=3+flr(rnd(5))
			idx=max(idx,3+flr(rnd(5)))
			mset(c,r,idx)
		end
	end
	-- copy left quadrant to right
	for r=0,_qsz-1 do
		for c=0,_qsz-1 do
			local idx=mget(c,r)
			mset(_qsz+c,r,idx)
		end
	end
	-- copy top half to bottom
	for r=0,_qsz-1 do
		for c=0,_siz-1do
			local idx=mget(c%_qsz,r)
			mset(c,_qsz+r,idx)
		end
	end
end

function tget(r,c)
	local res="="
	if r<=#_data then
		local cols=#_data[r]
		c=1+(c-1)%cols
		res=sub(_data[r],c,c)
	end
	return res
end

function tset(r,c,v)
	local cols=#_data[r]
	c=1+(c-1)%cols
	local text=sub(_data[r],1,c-1)
	text=text..v
	text=text..sub(_data[r],c+1,-1)
	_data[r]=text
end

function round(n)
	local dn=flr(n)
	local up=ceil(n)
	local res=up
	if abs(n-dn)<abs(n-up) then
		res=dn
	end
	return res
end

function _update60()
	if _moving or _done then
	else
		if btnp(⬅️) then
			_rt=max(_mnrt,_rt-2)
		elseif btnp(➡️) then
			_rt=min(_mxrt,_rt+2)
		end
		if btnp(⬆️) then
			_dn=max(_mndn,_dn-1)
		elseif btnp(⬇️) then
			_dn=min(_mxdn,_dn+1)
		end
	end
	if btnp(❎) or btnp(🅾️) then
		if _moving or _done then
			_init()
		else
			_moving=_y<32700
			local mp=_rt*_dn
			if mp==1 then
				_spd=2
			elseif mp<6 then
				_spd=1
			else
				_spd=0.5
			end
		end
	end
	local row=round(_y/_spc)
	if _y>32700 or row>#_data then
		_moving=false
		_done=true
	end
	local vx=_spd*_rt
	local vy=_spd*_dn
	if _moving then
		_x+=vx
		_y+=vy
	end
	-- did we hit a tree?
	local c=_x/_spc
	local r=_y/_spc
	local nr=round(r)
	local nc=round(c)
	local diff=max(
		abs(nc-c),
		abs(nr-r)
		)
	if diff<0.1 then
		local cel=tget(nr+1,nc+1)
		if cel=="#" then
			tset(nr+1,nc+1,"x")
			_hits+=1
			_shk=45
			sfx(0)
			local amt=27
			if #_debris>128 then
				amt=9
			elseif #_debris>64 then
				amt=18
			end
			for i=1,amt do
				local s=19+flr(rnd(5))
				s=min(s,19+flr(rnd(5)))
				add(_debris,
					{
						s=s,
						x=_x,
						y=_y,
						dx=rnd(3)-0.5,
						dy=rnd(3)-0.5,
						tm=60+flr(rnd(60)),
						vs=true,
						fx=rnd()<0.5,
						fy=rnd()<0.5,
						}
					)
			end
		end
	end
	-- shake the screen?
	_shk=max(0,_shk-1)
	local shk=flr(_shk/30)
	if rnd()<0.5 then
		_cx=flr(rnd(2*shk+1))-shk
	else
		_cy=flr(rnd(2*shk+1))-shk
	end
	for d in all(_debris) do
		d.x+=d.dx
		d.y+=d.dy
		d.tm-=1
		d.vs=not d.vs
	end
end

function _draw()
	cls(1)
	-- draw snow
	local m=8*_siz/2
	local x=_x%m
	local y=_y%m
	camera(x+_cx,y+_cy)
	map(0,0,0,0,128,128)
	-- draw trees
	camera(_x-64+_cx,_y-64+_cy)
	local rows=#_data
	local cols=#_data[1]
	for r=0,31 do
		for c=0,31 do
			local tr=flr(_y/_spc)-16
			local tc=flr(_x/_spc)-16
			local col=1+(tc+c)%cols
			if tr+r+1<1 then
				if tr+r+1==0 then
					local sy=_spc*(r+tr)
					local sx=_spc*(c+tc)
					spr(34,sx-4,sy-4)
				end
			elseif tr+r+1>#_data then
				if tr+r==#_data then
					local sy=_spc*(r+tr)
					local sx=_spc*(c+tc)
					spr(34,sx-4,sy-4)
				end
			else
				local row=_data[tr+r+1]
				local cel=sub(row,col,col)
				local sy=_spc*(r+tr)
				local sx=_spc*(c+tc)
				if cel=="#" then
					spr(2,sx-4,sy-4)
					--rect(sx-1,sy-1,sx,sy,8)
				elseif cel=="x" then
					spr(18,sx-4,sy-4)
				end
			end
		end
	end
	-- draw debris
	camera(_x-64,_y-64)
	for d in all(_debris) do
		if d.tm<0 then
			del(_debris,d)
		elseif d.vs or d.tm>30 then
			spr(
				d.s,d.x-4,d.y-4,
				1,1,
				d.fx,d.fy
				)
		end
	end
	-- draw toboggan
	camera(x-64+_cx,y-64+_cy)
	local idx=1
	if not _moving then
		idx=33
	elseif flr(4*t()%2)<1 then
		idx=17
	end
	spr(idx,x-4,y-4)
	-- draw target
	idx=16
	if t()%1<0.5 then
		idx=32
	end
	x+=8*_rt
	y+=8*_dn
	spr(idx,x+_cx,y+_cy)
	idx=35
	if t()%1<0.5 then
		idx+=16
	end
	if _moving or _done then
	else
		spr(idx,x+_cx+8,y+_cy)
		spr(idx+1,x+_cx,y+_cy+8)
	end
	-- draw progress bar
	camera()
	spr(9,1,9)
	local y=9
	local ht=8
	for i=1,12 do
		spr(25,1,y+ht)
		ht+=8
